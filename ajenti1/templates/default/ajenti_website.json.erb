{
    "ssl_header": "",
    "maintenance_mode": false,
    "locations": [],
    "custom_conf": "location / {\n\ttry_files $uri $uri/ /index.php?$args;\n}\nlocation ^~ /data {\n  deny all;\n}",
    "ssl_stapling_verify": "",
    "owner": "root",
    "custom_conf_toplevel": "server {\n        listen       80;\n        server_name  <%= @domain %>;\n        rewrite ^ https://$http_host$request_uri? permanent;    # force redirect http to https\n        server_tokens off;\n}",
    "ssl_session_cache": "",
    "ssl_key_path": "<%= node['ajenti1']['website_ssl_key'] %>",
    "ssl_protocols": "",
    "ssl_prefer_server_ciphers": "",
    "ssl_diffie_hellman_group": "",
    "ssl_session_timeout": "",
    "ssl_cert_path": "<%= node['ajenti1']['website_ssl_cert'] %>",
    "name": "website",
    "ssl_ciphers": "",
    "ssl_stapling": "",
    "enabled": true,
    "extensions": {
        "ajenti.plugins.vh.processes.ProcessesExtension": {
            "processes": []
        }
    },
    "domains": [
        {
            "domain": "<%= @domain %>"
        }
    ],
    "root": "<%= node['ajenti1']['website_root'] %><%= @domain %>",
    "ports": [
        {
            "spdy": false,
            "default": false,
            "ssl": true,
            "host": "*",
            "http2": true,
            "port": 443
        }
    ]
}
